---
import SEO from "../components/SEO.astro";

interface Props {
  generator: string;
  title: string;
  description: string;
  websiteUrl: string;
  altImage: string;
}

const { generator, title, description, websiteUrl, altImage } =
  Astro.props as Props;
---

<html lang="en">
  <head>
    <link rel="stylesheet" href="/beanstreet/style.css" />
    <SEO {generator} {title} {description} {websiteUrl} {altImage} />
    <!-- Preload Font -->
    <link
      rel="preload"
      href="/fonts/boska-variable.woff2"
      as="font"
      type="font/woff2"
      crossorigin
    />
    <link
      rel="preload"
      href="/fonts/poppins-variable.woff2"
      as="font"
      type="font/woff2"
      crossorigin
    />
  </head>
  <body class="bg-beige-100">
    <slot />
  </body>
</html>
